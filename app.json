{
  "expo": {
    "name": "Love Together",
    "slug": "love-together",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B9D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
